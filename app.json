{
  "expo": {
    "name": "Lil Watan",
    "slug": "watan",
    "version": "1.0.0",
    "sdkVersion": "40.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "androidStatusBar": {
      "backgroundColor": "#0000",
      "translucent": false
    },
    "platforms": [
      "ios",
      "android"
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.lilwatan"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
